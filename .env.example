# OpenAI API Keys (replace with your actual keys)
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Retrieval API Configuration
OPENAI_ASSISTANT_ID=your_assistant_id_here
# If no assistant ID is provided, a new one will be created automatically

# Model Configuration
FINE_TUNED_MODEL=gpt-4o

# Optional Settings
EMBEDDING_MODEL=text-embedding-3-small
MAX_TOKENS=4096
RESPONSE_TOKENS=150
CONTEXT_TOKENS=1000

# Debug Settings (true/false)
DEBUG=false
PERFORMANCE_TRACKING=true 